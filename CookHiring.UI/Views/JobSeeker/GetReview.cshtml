@model IEnumerable<CookHiring.Data.Review>

@{
    ViewBag.Title = "GetReview";
    Layout = "~/Views/Shared/_JobSeekerDashboard.cshtml";
}

<h2>My Reviews</h2>


<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.Rate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.comment)
        </th>
        <th></th>
    </tr>



    @foreach (var item in Model)
    {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.Rate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.comment)
            </td>

        </tr>
    }

</table>
<div>
    @Html.ActionLink("Back to Home", "Dashboard")
</div>
